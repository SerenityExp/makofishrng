-- Create the main GUI
local player = game.Players.LocalPlayer
local gui = Instance.new("ScreenGui")
gui.Parent = player.PlayerGui

-- Create the main frame
local mainFrame = Instance.new("Frame")
mainFrame.Size = UDim2.new(0, 400, 0, 400) -- Increased width for Teleports Section
mainFrame.Position = UDim2.new(1, -410, 0.5, -200) -- Positioned on the right side
mainFrame.BackgroundColor3 = Color3.new(0, 0, 0) -- Black background
mainFrame.BorderSizePixel = 0
mainFrame.Parent = gui

-- Add glowing blue border
local border = Instance.new("UIStroke")
border.Thickness = 3
border.Color = Color3.new(0, 0.5, 1) -- Blue color
border.Parent = mainFrame

-- Add glow effect to the border
local glow = Instance.new("UIGradient")
glow.Rotation = 90
glow.Color = ColorSequence.new({
    ColorSequenceKeypoint.new(0, Color3.new(0, 0.5, 1)), -- Blue
    ColorSequenceKeypoint.new(1, Color3.new(0, 1, 1)) -- Cyan
})
border.Transparency = NumberSequence.new(0.5) -- Slight transparency for glow effect

-- Make the frame draggable
local UserInputService = game:GetService("UserInputService")
local dragging
local dragInput
local dragStart
local startPos

local function update(input)
    local delta = input.Position - dragStart
    mainFrame.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
end

mainFrame.InputBegan:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
        dragging = true
        dragStart = input.Position
        startPos = mainFrame.Position

        input.Changed:Connect(function()
            if input.UserInputState == Enum.UserInputState.End then
                dragging = false
            end
        end)
    end
end)

mainFrame.InputChanged:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
        dragInput = input
    end
end)

UserInputService.InputChanged:Connect(function(input)
    if input == dragInput and dragging then
        update(input)
    end
end)

